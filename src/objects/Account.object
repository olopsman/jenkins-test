    <validationRules>
        <fullName>CSO_SA_SysAdmin_Transfer_Ownership</fullName>
        <active>true</active>
        <description>Only CSO, Sales Admin and System Admin should be able to transfer ownership of accounts.All other roles should not be able to transfer ownership of any accounts.</description>
        <errorConditionFormula>AND(
ISCHANGED( OwnerId ),
$Profile.Name &lt;&gt; &quot;System Administrator&quot;,
$Profile.Name &lt;&gt; &quot;Chief Sales Officer&quot;,
$Profile.Name &lt;&gt; &quot;Sales Administrator&quot;)</errorConditionFormula>
        <errorMessage>Only CSO, Sales Admin and System Admin can transfer ownership of accounts</errorMessage>
    </validationRules>
