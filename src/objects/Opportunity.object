    <validationRules>
        <fullName>CSO_SA_SysAdmin_Transfer_Ownership</fullName>
        <active>true</active>
        <description>Only CSO, Sales Admin and System Admin should be able to transfer ownership of opportunities.All other roles should not be able to transfer ownership of any opportunities.</description>
        <errorConditionFormula>AND(
ISCHANGED( OwnerId ),
$Profile.Name &lt;&gt; &quot;System Administrator&quot;,
$Profile.Name &lt;&gt; &quot;Chief Sales Officer&quot;,
$Profile.Name &lt;&gt; &quot;Sales Administrator&quot;)</errorConditionFormula>
        <errorMessage>Only CSO, Sales Admin and System Admin can transfer ownership of opportunities</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Close_Comments_entered_if_Other</fullName>
        <active>true</active>
        <description>Comments must be added in the &apos;Close Comments&apos; field if &apos;Other&apos; is selected as &apos;Close Reason&apos;.</description>
        <errorConditionFormula>AND(
 ISPICKVAL(Lost_Reason__c,&quot;Other&quot;),
 ISBLANK(Close_Comments__c )
)</errorConditionFormula>
        <errorDisplayField>Close_Comments__c</errorDisplayField>
        <errorMessage>Comments must be added in the &apos;Close Comments&apos; field if &apos;Other&apos; is selected as &apos;Close Reason&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Close_Reason_must_be_selected_if_Lost</fullName>
        <active>true</active>
        <description>A &apos;Close Reason&apos; must be selected if the Opportunity is set to Closed Lost.</description>
        <errorConditionFormula>AND(
 OR(
  ISPICKVAL(StageName,&quot;Closed - Lost&quot;),
  ISPICKVAL(StageName,&quot;Closed - Deferred&quot;)
    ),
 ISBLANK(TEXT(Lost_Reason__c))
)</errorConditionFormula>
        <errorDisplayField>Lost_Reason__c</errorDisplayField>
        <errorMessage>A &apos;Close Reason&apos; must be selected if the Opportunity is set to Closed Lost.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Contract_Must_Be_Attached_if_Closed_Won</fullName>
        <active>true</active>
        <description>&apos;Contract Attached&apos; field must be checked for an Opportunity to be &apos;Closed - Won&apos;</description>
        <errorConditionFormula>AND(
 
 ISPICKVAL( StageName , &quot;Closed - Won&quot;),
 Contract_Attached__c = FALSE

)</errorConditionFormula>
        <errorDisplayField>Contract_Attached__c</errorDisplayField>
        <errorMessage>&apos;Contract Attached&apos; field must be checked for an Opportunity to be &apos;Closed - Won&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opportunity_Qualification</fullName>
        <active>true</active>
        <description>TAS 1-20 Assessment has to be completed before setting to &quot;Qualified Opportunity&quot;.</description>
        <errorConditionFormula>AND(
OR(
 DM_Assessment_Count__c &lt; 20,
 ISBLANK(DM_Assessment_Count__c)
),
 ISPICKVAL(StageName , &quot;Qualified Opportunity&quot;)
)</errorConditionFormula>
        <errorMessage>TAS 1-20 Assessment has to be completed before setting to &apos;Qualified Opportunity&apos;.</errorMessage>
    </validationRules>
